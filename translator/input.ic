int a,b,c;

void some_function()
{
	a+=b-c;
}

void another_function()
{
	1;
}

/*Blink process*/
process Proc1 : background
{


	state FS_START 
	{	
		{
			if(a<b)
			{
				c = a + b;
				_delay_us(50);
			}
		
			some_function(a,a,a,a);
			
			b++;
			b--;
		}
	
		DDRB |= (1<<5);
		to state LED_ON;	
	}
	
	state LED_ON 
	{
	
		timeout(500)
		{
			PORTB &= ~(1<<5);
			to state LED_OFF;	
		}
	}
	
	state LED_OFF
	{
		timeout(500)
		{
			PORTB |= (1<<5);
			to state LED_ON;
		}
	}
}

